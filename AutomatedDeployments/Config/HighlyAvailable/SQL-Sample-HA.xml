<?xml version="1.0" encoding="utf-8"?>
<Azure SubscriptionName="{SUBSCRIPTION NAME}" ServiceName="{SERVICE NAME}" StorageAccount="{STORAGE ACCOUNT}" VNetName="SPVNet" AffinityGroup="SPVNet-AG">
  <ServiceAccounts>
	  <ServiceAccount UserName="SPAdmin" Password="{PASSWORD}" Type="WindowsLocal" Create="No" /> <!--Account used to create VMs-->
	  <ServiceAccount UserName="CORP\SPAdmin" Password="{PASSWORD}" Type="WindowsDomain" Create="No" /> <!--Account used to connect to AD-->
	  <ServiceAccount UserName="spdbaccess" Password="{PASSWORD}" Type="SQL" />
	  <ServiceAccount UserName="CORP\SQLSvc1" Password="{PASSWORD}" Type="WindowsDomain" />
	  <ServiceAccount UserName="CORP\SQLSvc2" Password="{PASSWORD}" Type="WindowsDomain" />
  </ServiceAccounts>
  <Connections>
    <ActiveDirectory Domain="CORP" DnsDomain="corp.contoso.com" DomainControllerVM="DC1" ServiceAccountName="CORP\SPAdmin" ServiceName="{AD SERVICE NAME}"/>
  </Connections>
  <AzureVMGroups>
    <VMRole Name="SQLServers" HighAvailabilityType="SQLAlwaysOn" VMSize="Medium"
            StartingImageName="{SQL SERVER 2012 ENTERPRISE IMAGE NAME}"
            QuorumStartingImageName="{WINDOWS SERVER 2012 IMAGE NAME}"
            DataDiskSizesInGB="Data:20;Log:10" DefaultSQLDataFolder="F:\Data" DefaultSQLLogFolder="G:\Logs" AvailabilitySet="SQLHA" VNetName="SPVNet" 
            SubnetNames="AppSubnet" ServiceAccountName="SPAdmin">
      <AzureVM Name="SQLQ" Type="QUORUM" />
      <AzureVM Name="SQLS" Type="SECONDARY" />
      <AzureVM Name="SQLP" Type="PRIMARY" /><!--Please ensure that the Type Primary is the last entry for the AzureVM-->
    </VMRole>
  </AzureVMGroups>
  <SQLCluster Name="WSFCluster" 
              SQLAvailabilityGroup="AG1"
              InstallerDomainUsername="CORP\SPAdmin"
              InstallerDatabaseUserName="spdbaccess"
              PrimaryServiceAccountName="CORP\SQLSvc1"
              SecondaryServiceAccountName="CORP\SQLSvc2"
              FailoverTestDBName="TestDB"
              />
</Azure>